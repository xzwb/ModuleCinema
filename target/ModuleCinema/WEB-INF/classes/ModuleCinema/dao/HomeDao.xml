<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ModuleCinema.dao.HomeDao">

    <select id="homeDao" resultType="ModuleCinema.pojo.Play">
        select *from play where playState = 1 limit 10;
    </select>

    <select id="selectStaff" resultType="ModuleCinema.pojo.Staff">
        select *from staff order by staffId limit #{start}, 10;
    </select>

    <select id="staffTotal" resultType="int">
        select count(*) from staff;
    </select>
</mapper>