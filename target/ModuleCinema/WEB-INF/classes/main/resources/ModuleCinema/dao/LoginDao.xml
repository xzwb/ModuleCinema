<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ModuleCinema.dao.LoginDao">
    <select id="judgeLoginInformation" parameterType="ModuleCinema.pojo.User" resultType="ModuleCinema.pojo.User">
        select *from user where userId = #{userId} and userPassword = #{userPassword};
    </select>
</mapper>