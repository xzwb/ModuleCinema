<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ModuleCinema.dao.RegisterDao">
    <update id="registerUpdate" useGeneratedKeys="true" keyProperty="userId" parameterType="ModuleCinema.pojo.User">
        insert into user values(default, #{userName}, '', 3, #{userPassword}, #{userSex}, #{birthday}, #{securityQuestion}, #{classifiedAnswer});
    </update>

    <select id="registerSelectById" resultType="ModuleCinema.pojo.User" parameterType="int">
        select *from user where userId = #{id};
    </select>
</mapper>